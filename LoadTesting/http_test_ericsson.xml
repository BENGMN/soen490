<?xml version="1.0"?>
<!DOCTYPE tsung SYSTEM "/usr/share/tsung/tsung-1.0.dtd">
<tsung loglevel="notice" version="1.0">
  <clients><client host="localhost" use_controller_vm="true"/></clients>
  <servers><server host="192.168.1.150" port="80" type="tcp"></server></servers>
  <!--<monitoring><monitor host="localhost" type="snmp"></monitor></monitoring>-->
  
  <!-- <load duration="2" unit="minute"> -->
  <load unit="second">
   <arrivalphase phase="1" duration="1" unit="second">
